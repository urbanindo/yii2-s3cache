{
    "name": "urbanindo/yii2-s3cache",
    "description": "Yii2 Component for Caching in S3",
    "license": "MIT",
    "authors": [
        {
            "name": "Petra Barus",
            "email": "petra.barus@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "aws/aws-sdk-php": "^3.28.0"
    },
    "require-dev": {
        "phing/phing": "2.*",
        "phpunit/phpunit": "4.6.*",
        "phpunit/dbunit": ">=1.2",
        "phpunit/php-code-coverage": "2.2.4",
        "fzaninotto/faker": "dev-master",
